<div class="trading" v-if="false">
    <div class="card card-fixed-large mx-auto mb-3">
        <div class="card-body p-0">
            <div class="card-header">
                <span>
                    {% trans %} trading.mintme_markets {% endtrans %}
                </span>
            </div>
            <div class="row coin-markets d-flex justify-content-center align-items-center">
                <div class="icon-loading"></div>
            </div>
        </div>
    </div>
    <div class="card card-fixed-large mx-auto">
        <div class="card-body p-0">
          <div class="token-trading-title card-header d-flex flex-wrap align-items-center px-0 pb-0">
             <span class="px-3 pb-2 mr-auto">
                    {% trans %} trading.tokens {% endtrans %}
             </span>
             <div>
                <div id="customFilter" class="dropdown b-dropdown px-3 pb-2 btn-group" value="deployed">
                   <button
                       id="customFilter__BV_toggle_"
                       aria-haspopup="true"
                       aria-expanded="false"
                       type="button"
                       class="btn dropdown-toggle btn-primary"
                    >
                        <span>
                           {% trans %} trading.deployed.label {% endtrans %}
                       </span>
                   </button>
                </div>
             </div>
            </div>
            <div slot="title" class="card-title font-weight-bold pl-3 pt-3 pb-1">
                <span class="float-left">
                    {{ tokensCount }} {% trans %} trading.tokens_and_market_cap {% endtrans %}
                    <div class="icon-loading"></div>
                </span>
            </div>
            <div class="trading-table table-responsive text-nowrap">
                <table role="table" aria-busy="false" aria-colcount="5" class="table b-table" id="__BVID__42">
                    <thead role="rowgroup" class="trading-head">
                       <tr role="row" class="">
                          <th
                              role="columnheader"
                              scope="col"
                              tabindex="0"
                              aria-colindex="1"
                              aria-sort="none"
                              class="pair-cell-trading b-table-sort-icon-left"
                            >
                             <div>
                                 {% trans %} trading.fields.pair {% endtrans %}
                             </div>
                          </th>
                          <th
                              role="columnheader"
                              scope="col"
                              tabindex="0"
                              aria-colindex="2"
                              aria-sort="none"
                              class="b-table-sort-icon-left"
                           >
                             <div>
                                 {% trans %} trading.fields.change {% endtrans %}
                             </div>
                          </th>
                          <th role="columnheader" scope="col" tabindex="0" aria-colindex="3" aria-sort="none" class="b-table-sort-icon-left">
                              {% trans %} trading.fields.last_price {% endtrans %}
                          </th>
                          <th role="columnheader" scope="col" tabindex="0" aria-colindex="4" aria-sort="none" class="b-table-sort-icon-left">
                             <div id="volume" class="dropdown b-dropdown position-static btn-group">
                                <button id="volume__BV_toggle_" aria-haspopup="true" aria-expanded="false" type="button" class="btn dropdown-toggle btn-primary">
                                  {% trans %} trading.month_volume.label {% endtrans %}
                                </button>
                             </div>
                             <div class="d-inline ml-1 mr-2">
                                <a data-tippy="">
                                   <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="question" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 384 512" slot="icon" class="ml-1 text-white
                                      rounded-circle square blue-question svg-inline--fa fa-question fa-w-12">
                                      <path fill="currentColor" d="M202.021 0C122.202 0 70.503 32.703 29.914 91.026c-7.363 10.58-5.093 25.086 5.178 32.874l43.138 32.709c10.373 7.865 25.132 6.026 33.253-4.148 25.049-31.381 43.63-49.449 82.757-49.449 30.764 0 68.816 19.799 68.816 49.631 0 22.552-18.617 34.134-48.993 51.164-35.423 19.86-82.299 44.576-82.299 106.405V320c0 13.255 10.745 24 24 24h72.471c13.255 0 24-10.745 24-24v-5.773c0-42.86 125.268-44.645 125.268-160.627C377.504 66.256 286.902 0 202.021 0zM192 373.459c-38.196 0-69.271 31.075-69.271 69.271 0 38.195 31.075 69.27 69.271 69.27s69.271-31.075 69.271-69.271-31.075-69.27-69.271-69.27z" class=""></path>
                                   </svg>
                                </a>
                             </div>
                          </th>
                          <th role="columnheader" scope="col" tabindex="0" aria-colindex="5" aria-sort="none" class="b-table-sort-icon-left">
                             <div id="marketCap" class="dropdown b-dropdown position-static btn-group">
                                <button id="marketCap__BV_toggle_" aria-haspopup="true" aria-expanded="false" type="button" class="btn dropdown-toggle btn-primary">
                                    {% trans %} trading.buy_depth.label {% endtrans %}
                                </button>
                             </div>
                             <div class="d-inline ml-1 mr-2">
                                <a data-tippy="">
                                   <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="question" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 384 512" slot="icon" class="ml-1 text-white
                                      rounded-circle square blue-question svg-inline--fa fa-question fa-w-12">
                                      <path fill="currentColor" d="M202.021 0C122.202 0 70.503 32.703 29.914 91.026c-7.363 10.58-5.093 25.086 5.178 32.874l43.138 32.709c10.373 7.865 25.132 6.026 33.253-4.148 25.049-31.381 43.63-49.449 82.757-49.449 30.764 0 68.816 19.799 68.816 49.631 0 22.552-18.617 34.134-48.993 51.164-35.423 19.86-82.299 44.576-82.299 106.405V320c0 13.255 10.745 24 24 24h72.471c13.255 0 24-10.745 24-24v-5.773c0-42.86 125.268-44.645 125.268-160.627C377.504 66.256 286.902 0 202.021 0zM192 373.459c-38.196 0-69.271 31.075-69.271 69.271 0 38.195 31.075 69.27 69.271 69.27s69.271-31.075 69.271-69.271-31.075-69.27-69.271-69.27z" class=""></path>
                                   </svg>
                                </a>
                             </div>
                          </th>
                           <th role="columnheader" scope="col" tabindex="0" aria-colindex="5" aria-sort="none" class="b-table-sort-icon-left">
                               {% trans %} trading.fields.holders {% endtrans %}
                               <div class="d-inline ml-1 mr-2">
                                   <a data-tippy="">
                                       <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="question" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 384 512" slot="icon" class="ml-1 text-white
                                      rounded-circle square blue-question svg-inline--fa fa-question fa-w-12">
                                           <path fill="currentColor" d="M202.021 0C122.202 0 70.503 32.703 29.914 91.026c-7.363 10.58-5.093 25.086 5.178 32.874l43.138 32.709c10.373 7.865 25.132 6.026 33.253-4.148 25.049-31.381 43.63-49.449 82.757-49.449 30.764 0 68.816 19.799 68.816 49.631 0 22.552-18.617 34.134-48.993 51.164-35.423 19.86-82.299 44.576-82.299 106.405V320c0 13.255 10.745 24 24 24h72.471c13.255 0 24-10.745 24-24v-5.773c0-42.86 125.268-44.645 125.268-160.627C377.504 66.256 286.902 0 202.021 0zM192 373.459c-38.196 0-69.271 31.075-69.271 69.271 0 38.195 31.075 69.27 69.271 69.27s69.271-31.075 69.271-69.271-31.075-69.27-69.271-69.27z" class=""></path>
                                       </svg>
                                   </a>
                               </div>
                           </th>
                       </tr>
                    </thead>
                    <tbody role="rowgroup" class="trading-body">
                        {% for market in markets|slice(3) %}
                           <tr role="row">
                              <td aria-colindex="1" role="cell" class="pair-cell-trading">
                                 <div>
                                     <a href="{{ path('token_show', {name: market.quote.name}) }}" class="text-white">
                                       <div tabindex="-1" class="avatar d-inline avatar__small">
                                           <img src="{{ market.quote.image.avatar_small}}" class="rounded-circle img-fluid">
                                       </div>
                                       {{ market.quote.name }}
                                    </a>
                                   {% if market.quote.deploymentStatus == 'deployed'%}
                                        <div class="d-inline">
                                           <a data-tippy="">
                                               <img src="{{ asset('/media/default_mintme.png') }}">
                                           </a>
                                        </div>
                                   {% endif %}
                                 </div>
                              </td>
                              <td aria-colindex="2" role="cell" class="">
                                    <div class="icon-loading ml-2"></div>
                              </td>
                              <td aria-colindex="3" role="cell" class="">
                                    <div class="icon-loading ml-2"></div>
                              </td>
                              <td aria-colindex="4" role="cell" class="">
                                    <div class="icon-loading ml-2"></div>
                              </td>
                              <td aria-colindex="5" role="cell" class="">
                                    <div class="icon-loading ml-2"></div>
                              </td>
                              <td aria-colindex="6" role="cell" class="">
                                  {{ market.quote.holdersCount }}
                              </td>
                           </tr>
                        {% endfor %}
                    </tbody>
                </table>
          </div>
          <div class="row justify-content-center"><a href="#" target="_self" class="pointer-events-none">Show all tokens</a></div>
          <div class="row justify-content-center">
             <ul role="menubar" aria-disabled="false" aria-label="Pagination" class="pagination my-0 b-pagination">
                <li role="presentation" aria-hidden="true" class="page-item disabled"><span role="menuitem" aria-label="Go to first page" aria-disabled="true" class="page-link">«</span></li>
                <li role="presentation" aria-hidden="true" class="page-item disabled"><span role="menuitem" aria-label="Go to previous page" aria-disabled="true" class="page-link">‹</span></li>
                <li role="presentation" class="page-item active">
                    <button role="menuitemradio" type="button" aria-checked="true" aria-posinset="1" aria-setsize="1" tabindex="0" class="page-link">
                       ...
                    </button>
                </li>
                <li role="presentation" aria-hidden="true" class="page-item disabled"><span role="menuitem" aria-label="Go to next page" aria-disabled="true" class="page-link">›</span></li>
                <li role="presentation" aria-hidden="true" class="page-item disabled"><span role="menuitem" aria-label="Go to last page" aria-disabled="true" class="page-link">»</span></li>
             </ul>
          </div>
       </div>
    </div>
</div>
