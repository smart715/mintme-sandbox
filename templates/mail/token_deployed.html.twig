{% extends 'mail/layout.html.twig' %}
{% block content %}
    <table class="body"
           style="Margin:0;background:#f3f3f3!important;border-collapse:collapse;border-spacing:0;color:#0a0a0a;font-family:Helvetica,Arial,sans-serif;font-size:16px;font-weight:400;height:100%;line-height:1.3;margin:0;padding-bottom:0;padding-left:0;padding-right:0;padding-top:0;text-align:left;vertical-align:top;width:100%">
        <tbody>
        <tr style="padding-bottom:0;padding-left:0;padding-right:0;padding-top:0;text-align:left;vertical-align:top">
            <td align="center" class="center"
                style="-moz-hyphens:auto;-webkit-hyphens:auto;Margin:0;border-collapse:collapse!important;color:#0a0a0a;font-family:Helvetica,Arial,sans-serif;font-size:16px;font-weight:400;hyphens:auto;line-height:1.3;margin:0;padding-bottom:0;padding-left:0;padding-right:0;padding-top:0;text-align:left;vertical-align:top;word-wrap:break-word"
                valign="top">
                <div style="width:100%;max-width:600px;Margin:0;padding-top:0;padding-right:0;padding-bottom:0;padding-left:0;display:inline-block;vertical-align:middle;">
                    <table role="presentation" width="100%" border="0" cellpadding="0" cellspacing="0"
                           style="border-collapse:collapse;background-color:#ffffff;" bgcolor="#ffffff">
                        <tr>
                            <td align="center" width="600"
                                style="width:100%;Margin:0;padding-top:0;padding-right:0;padding-bottom:0;padding-left:0;line-height:16px;height:16px;background-color:#ffffff;"
                                bgcolor="#ffffff">
                                <img alt="Blockchain" src="{{ absolute_url(asset('build/images/Blockchain.png')) }}"
                                     width="120"
                                     style="width:120px;height:auto;border:none;outline:0;text-align:center;text-decoration:none;color:#ffffff;-ms-interpolation-mode:bicubic;"/>
                                <p style="Margin:0 0 12px 0;padding:0;word-break:break-all;text-align:center;vertical-align:middle;font-family:Helvetica,Arial,sans-serif;font-size:24px;font-weight:400;line-height:26px;color:#0a0a0a;">{% trans from 'messages' %} hello {% endtrans %}
                                </p>
                                <p style="Margin:0 0 18px 0;padding:0;word-break:break-all;text-align:center;vertical-align:middle;font-family:Helvetica,Arial,sans-serif;font-size:30px;font-weight:400;line-height:30px;color:#2199e8;">{{ username }}
                                </p>
                                <p style="Margin:0 0 0 0;padding:0;text-align:center;vertical-align:middle;font-family:Helvetica,Arial,sans-serif;font-size:20px;font-weight:400;line-height:26px;color:#0a0a0a;">
                                    {% trans %}mail.token_deployed.p_1{% endtrans %}
                                </p>
                                <table role="presentation" width="100%" border="0" cellpadding="0" cellspacing="0">
                                    <tr>
                                        <td align="center" style="Margin:0;padding-top:0;padding-right:0;padding-bottom:0;padding-left:0;line-height:24px;height:48px;">
                                            <p style="Margin:4px;padding:0;text-align:center;vertical-align:middle;font-family:Helvetica,Arial,sans-serif;font-size:15px;font-weight:600;line-height:24px;color:#0a0a0a;word-break:break-word;">
                                                <a href="{{ explorerUrl }}" target="_blank">{{ explorerUrl }}</a>
                                            </p>
                                        </td>
                                    </tr>
                                </table>
                                <p style="Margin:0 0 0 0;padding:0;text-align:center;vertical-align:middle;font-family:Helvetica,Arial,sans-serif;font-size:20px;font-weight:400;line-height:26px;color:#0a0a0a;">
                                    {% trans%}mail.token_deployed.p_1.a{% endtrans %}
                                </p>
                                <ol style="text-align: left;font-family: Helvetica,Arial,sans-serif;font-size: 17px;margin-top: 7px;">
                                    <li>{% trans %}mail.token_deployed.p_1.1{% endtrans %}</li>
                                    <li>{% trans %}mail.token_deployed.p_1.2{% endtrans %}</li>
                                    <li>{% trans %}mail.token_deployed.p_1.3{% endtrans %}</li>
                                </ol>
                                <p style="Margin:0 20px 0 20px;padding:0;text-align:center;vertical-align:middle;font-family:Helvetica,Arial,sans-serif;font-size:20px;font-weight:400;line-height:26px;color:#0a0a0a;">
                                    {% trans %}mail.token_deployed.p_2{% endtrans %}
                                </p>
                                <ol style="text-align: left;font-family: Helvetica,Arial,sans-serif;font-size: 17px;margin-top: 7px;">
                                    <li>{% trans %}mail.token_deployed.p_2.1{% endtrans %}</li>
                                    <li>{% trans %}mail.token_deployed.p_2.2{% endtrans %}</li>
                                    <li>{% trans %}mail.token_deployed.p_2.3{% endtrans %}</li>
                                </ol>
                                <p style="Margin:0 20px 0 20px;padding:0;text-align:center;vertical-align:middle;font-family:Helvetica,Arial,sans-serif;font-size:20px;font-weight:400;line-height:26px;color:#0a0a0a;">
                                    {% trans %}mail.token_deployed.p_3{% endtrans %}
                                </p>
                                <p style="Margin:0 0 20px 0;padding:0;text-align:center;vertical-align:middle;font-family:Helvetica,Arial,sans-serif;font-size:16px;font-weight:600;line-height:8px;color:#0a0a0a;">
                                <ul style="text-align:left;">
                                    <li>
                                        <a href="{{ tokenSalesUrl }}"
                                           style="Margin:0;padding:0;font-weight:600;color:#2199e8;text-decoration:none;"
                                           target="_blank">{% trans %}mail.token_deployed.p_3.1{% endtrans %}
                                        </a>
                                    </li>&nbsp;
                                    <li>
                                        <a href="{{ aimingUrl }}"
                                           style="Margin:0;padding:0;font-weight:600;color:#2199e8;text-decoration:none;"
                                           target="_blank">{% trans %}mail.token_deployed.p_3.2{% endtrans %}
                                        </a>
                                    </li>&nbsp;
                                    <li>
                                        <a href="{{ ideasUrl }}"
                                           style="Margin:0;padding:0;font-weight:600;color:#2199e8;text-decoration:none;"
                                           target="_blank">{% trans %}mail.token_deployed.p_3.3{% endtrans %}
                                        </a>
                                    </li>&nbsp;
                                    <li>
                                        <a href="{{ stuckUrl }}"
                                           style="Margin:0;padding:0;font-weight:600;color:#2199e8;text-decoration:none;"
                                           target="_blank">{% trans %}mail.token_deployed.p_3.4{% endtrans %}
                                        </a>
                                    </li>&nbsp;
                                    <li>
                                        <a href="{{ talkingUrl }}"
                                           style="Margin:0;padding:0;font-weight:600;color:#2199e8;text-decoration:none;"
                                           target="_blank">{% trans %}mail.token_deployed.p_3.5{% endtrans %}
                                        </a>
                                    </li>
                                </ul>
                                </p>
                            </td>
                        </tr>
                    </table>
                </div>
            </td>
        </tr>
        </tbody>
    </table>
{% endblock %}

