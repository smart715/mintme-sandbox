<link rel="apple-touch-icon" sizes="114x114" href="{{ asset('build/images/apple-icon-114x114.png') }}">
<link rel="apple-touch-icon" sizes="120x120" href="{{ asset('build/images/apple-icon-120x120.png') }}">
<link rel="apple-touch-icon" sizes="144x144" href="{{ asset('build/images/apple-icon-144x144.png') }}">
<link rel="apple-touch-icon" sizes="152x152" href="{{ asset('build/images/apple-icon-152x152.png') }}">
<link rel="apple-touch-icon" sizes="180x180" href="{{ asset('build/images/apple-icon-180x180.png') }}">
<link rel="icon" type="image/png" sizes="192x192"  href="{{ asset('build/images/android-icon-192x192.png') }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ asset('build/images/favicon-32x32.png') }}">
<link rel="icon" type="image/png" sizes="96x96" href="{{ asset('build/images/favicon-96x96.png') }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ asset('build/images/favicon-16x16.png') }}">
<link rel="manifest" href="/manifest.json">
<meta name="msapplication-TileColor" content="#ffffff">
<meta name="msapplication-TileImage" content="/ms-icon-144x144.png">
<meta name="theme-color" content="#ffffff">
<link rel="manifest" href="{{ asset('manifest.json') }}">

<meta name="theme-color" content="#141C29">

<meta property="og:title" content="{{ metaTitle | default('mintMe | create your own token, monetize yourself!') }}">
<meta property="og:site_name" content="Mintme">
<meta property="og:type" content="website">
<meta property="og:url" content="{{ app.request.uri }}">
<meta property="og:image" content="{{ metaImage | default(asset('build/images/view-logo.png')) | absolute_url }}">
<meta
    property="og:image:alt"
    content="{{ metaImageAlt | default('Mintme logo. Create your own token, monetize yourself!') }}"
>

<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="{{ app.request.uri }}">
<meta name="twitter:creator" content="Mintme">

<style>
    @font-face {
        font-family: 'ConcordBold';
        src: url({{ asset('build/fonts/Concord-Bold.ttf') }}) format('truetype');
    }

    @font-face {
        font-family: 'Concord';
        src: url({{ asset('build/fonts/Concord.otf') }}) format('opentype');
    }

    body {
        background-color: #0c1a29;
    }
</style>

<!-- Global site tag (gtag.js) - Google Analytics -->
<script defer async src="https://www.googletagmanager.com/gtag/js?id={{ google_tracking_id }}"></script>

<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    {% if enable_google_analytics  %}
        gtag('config', '{{ google_tracking_id }}');
    {% else %}
        gtag('config', 'GA_MEASUREMENT_ID', { 'send_page_view': false });
    {% endif %}
</script>

<!--Start of Tawk.to Script-->
<script type="text/javascript">
    var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
    Tawk_API.onLoad = function(){
        Tawk_API.setAttributes({
            'name'  : '{{ app.user.profile.nickname|default('') }}',
            'email' : '{{ app.user.username|default('') }}',
            'hash'  : '{{ app.user.getTawkHash(tawk_api_key)|default('') }}',
            'user' : '{{ app.user.username|default('') }}',
            'token' : '{{ app.user.getProfile().getToken().getName()|default('') }}',
            'first-name' : '{{ app.user.getProfile().getFirstName()|default('') }}',
            'last-name' : '{{ app.user.getProfile().getLastName()|default('') }}'
        }, function(error){});
    }
</script>
<script
    defer
    src="https://embed.tawk.to/5c61c14e7cf662208c950e23/default"
    charset="UTF-8"
    crossorigin="*"
></script>
<!--End of Tawk.to Script-->
<script>
    var doc = document.documentElement;
    doc.setAttribute("data-useragent",  navigator.userAgent);
    doc.setAttribute("data-platform", navigator.platform );
</script>
