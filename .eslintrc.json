{
    "extends": [
        "google",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "max-len": ["error", 120],
        "no-console": "error",
        "yoda": ["error", "always"],
        "no-return-await": "error",
        "complexity": ["warn", 8],
        "indent": ["error", 4, {"SwitchCase": 1, "ignoredNodes": ["TemplateLiteral"]}],
        "no-duplicate-imports": ["error", { "includeExports": true }],
        "prefer-const": "error"
    }
}
