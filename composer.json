{
    "name": "Mint me",
    "description": "Mintme panel, exchange platform that allow users to create and exchange their own tokens built on Webchain.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "friendsofsymfony/user-bundle": "^2.1",
        "graze/guzzle-jsonrpc": "^3.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "ramsey/uuid": "^3.8",
        "scheb/two-factor-bundle": "^3.12",
        "sensio/framework-extra-bundle": "^5.2",
        "sensiolabs/security-checker": "^4.1",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer": "^4.1",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "overtrue/phplint": "^1.1",
        "phpstan/phpstan": "^0.10.1",
        "phpstan/phpstan-doctrine": "^0.10.0",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpstan/phpstan-symfony": "^0.10.0",
        "slevomat/coding-standard": "~4.0",
        "squizlabs/php_codesniffer": "*",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        },
        "incenteev-parameters": {
            "file": "config/parameters.yaml"
        }
    }
}
