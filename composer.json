{
    "name": "abchosting/mintme",
    "description": "Mintme panel, exchange platform that allow users to create and exchange their own tokens built on Webchain.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "~7.2",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "cron/cron-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "friendsofsymfony/ckeditor-bundle": "^1.1",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "friendsofsymfony/rest-bundle": "^2.3",
        "friendsofsymfony/user-bundle": "^2.1",
        "fzaninotto/faker": "^1.8",
        "graze/guzzle-jsonrpc": "^3.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "moneyphp/money": "^3.1",
        "nelmio/api-doc-bundle": "^3.4",
        "oro/doctrine-extensions": "^1.2",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "pragmarx/random": "^0.2.2",
        "presta/sitemap-bundle": "^1.7",
        "ramsey/uuid": "^3.8",
        "scheb/two-factor-bundle": "^3.12",
        "sensio/framework-extra-bundle": "^5.2",
        "sensiolabs/security-checker": "^5.0",
        "sirprize/postal-code-validator": "^1.3",
        "sonata-project/admin-bundle": "^3.31",
        "sonata-project/block-bundle": "^3.12",
        "sonata-project/classification-bundle": "^3.6",
        "sonata-project/doctrine-orm-admin-bundle": "^3.4",
        "sonata-project/formatter-bundle": "^3.4",
        "sonata-project/intl-bundle": "^2.4",
        "sonata-project/media-bundle": "^3.10",
        "sonata-project/news-bundle": "^3.5",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer": "^4.1",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^4.1",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/yaml": "^4.1",
        "vasek-purchart/doctrine-date-time-immutable-types": "^1.0"
    },
    "require-dev": {
        "liuggio/fastest": "^1.6",
        "overtrue/phplint": "^1.1",
        "phpstan/phpstan": "^0.10.1",
        "phpstan/phpstan-doctrine": "^0.10.0",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpstan/phpstan-symfony": "^0.10.0",
        "slevomat/coding-standard": "~4.0",
        "squizlabs/php_codesniffer": "*",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*",
        "egeloen/ckeditor-bundle": "*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        },
        "incenteev-parameters": {
            "file": "config/parameters.yaml"
        }
    }
}
