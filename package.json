    "moduleFileExtensions": [
      "js",
      "vue",
      "coffee"
    ],
    "testRegex": "assets/tests/.+\\.test\\.js",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".*\\.(coffee)$": "<rootDir>/coffee.processor.js"
    },
    "transformIgnorePatterns": [
      "^node_modules(.(?!.*\\.coffee))*$"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "(chart\\.js|vue-chartjs/legacy)$": "<rootDir>/assets/tests/__mocks__/chartjs.js",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assets/tests/__mocks__/fileMock.js",
